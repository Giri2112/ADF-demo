{
	"name": "flowlet1",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          id as string,",
				"          name as string,",
				"          address as string,",
				"          reg_date as string,",
				"          position as string,",
				"          salary as integer,",
				"          branch_id as string",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 aggregate(groupBy(id,",
				"          name,",
				"          address,",
				"          reg_date,",
				"          position,",
				"          salary,",
				"          branch_id),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          id,",
				"          name,",
				"          position,",
				"          count",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 output(mapColumn(",
				"          id,",
				"          name,",
				"          position,",
				"          count",
				"     )) ~> output1"
			]
		}
	}
}