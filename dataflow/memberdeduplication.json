{
	"name": "memberdeduplication",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Member_depulication",
						"type": "DatasetReference"
					},
					"name": "Memberdp"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "memberdp",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "aggregate1"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          member_name as string,",
				"          member_address as string,",
				"          reg_date as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> Memberdp",
				"Memberdp aggregate(groupBy(member_name,",
				"          member_address,",
				"          reg_date),",
				"     count = count()) ~> aggregate1",
				"aggregate1 select(mapColumn(",
				"          member_name,",
				"          member_address,",
				"          reg_date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          emp_name as string,",
				"          position as string,",
				"          salary as string,",
				"          branch_id as string",
				"     ),",
				"     partitionFileNames:['Member_deduplicated.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}